#include <bits/stdc++.h> 

int uniquePaths(int m ,int n)

{

    // first observation the steps and the right and the left 

    // ncr combinations to find the possible paths

    int N = n+m-2;

    int r=m-1;

    int ans=1;

    for(int i=1 ; i<=r ; i++)

    {

        ans = ans*(N-r+i)/i;

    }

    return ans;

 

    

}

 
